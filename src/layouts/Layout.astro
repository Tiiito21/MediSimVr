---
import '@fontsource/roboto'

import { ViewTransitions } from 'astro:transitions';
const { title } = Astro.props;


---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="MediSimVR practica sin complicaciones" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/cardboards.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
		<script src="https://cdn.jsdelivr.net/npm/js-cookie@3/dist/js.cookie.min.js" defer></script>
		<ViewTransitions />
	</head>
	<body class="min-h-screen bg-lavender-900 text-text font-roboto">
		<slot />
		
	</body>

	<slot name="script"/>
</html>